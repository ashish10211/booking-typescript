{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "noUnusedLocals": false,
      "outDir": "../lib",
      "resolveJsonModule": true,
      "skipLibCheck": true
    },
    "include": ["server.ts"],
    "exclude": ["node_modules"]
}
